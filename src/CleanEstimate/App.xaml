                <Utils:SharedResourceDictionary Source="Icons/Speichern.xaml" />
                <Utils:SharedResourceDictionary Source="Icons/SpeichernUnter.xaml" />
                <Utils:SharedResourceDictionary Source="Icons/Objekt.xaml" />
                <Utils:SharedResourceDictionary Source="Icons/Kalkulation.xaml" />
                <Utils:SharedResourceDictionary Source="Icons/Drucken.xaml" />
                <Utils:SharedResourceDictionary Source="Icons/DruckenHUebersicht.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
